<div class="container" id="events-form">
  {{#let (component 'create/event-form') as |EventForm|}}
    {{ log nameErrors }}
    {{#if model.errors.nameErrors }} <p>Meeeeeeeeeeee</p> {{ log this.model }} {{/if}}
    <EventForm
      @submit={{"createEvent"}}
      @centers={{this.model.centers}}
      @errors={{model.errors}}>
    </EventForm>
  {{/let}}
</div>

{{outlet}}